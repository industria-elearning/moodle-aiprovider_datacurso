<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="ai/provider/datacurso/db" VERSION="20251029" COMMENT="XMLDB file for Moodle ai/provider/datacurso"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="aiprovider_datacurso_rl" COMMENT="Per-user per-service ratelimit rolling usage state">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" COMMENT="Primary key"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" DEFAULT="0" COMMENT="User id"/>
        <FIELD NAME="serviceid" TYPE="char" LENGTH="100" NOTNULL="true" SEQUENCE="false" COMMENT="Service identifier (e.g. local_coursegen)"/>
        <FIELD NAME="windowstart" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" DEFAULT="0" COMMENT="Window start timestamp"/>
        <FIELD NAME="tokensused" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" DEFAULT="0" COMMENT="Tokens used within window"/>
        <FIELD NAME="lastsync" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" DEFAULT="0" COMMENT="Last sync timestamp with remote history"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" DEFAULT="0" COMMENT="Creation time"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" DEFAULT="0" COMMENT="Last modified time"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        <KEY NAME="userid_serviceid_uix" TYPE="unique" FIELDS="userid, serviceid"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="userid_idx" UNIQUE="false" FIELDS="userid"/>
        <INDEX NAME="serviceid_idx" UNIQUE="false" FIELDS="serviceid"/>
      </INDEXES>
    </TABLE>
  </TABLES>
</XMLDB>
